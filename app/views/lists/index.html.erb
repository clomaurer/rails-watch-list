<h1>Here is a list of your lists</h1>

    <%= cl_image_tag("THE_IMAGE_ID_FROM_LIBRARY",
      width: 400, height: 300, crop: :fill) %>

    <%= cl_image_tag("IMAGE_WITH_FACE_ID",
      width: 150, height: 150, crop: :thumb, gravity: :face) %>

<ul>
  <% @lists.each do |list| %>
    <li><%= list.name %></li>
    <%= link_to "More Details", list_path(list), class: "btn btn-primary" %>
  <% end %>
</ul>

<%= link_to "Add a list", new_list_path, class: "btn btn-primary" %>



